#include <stdio.h>
int main() 
{
    int n,i;
    printf("Enter number of elements in the array: ");
    scanf("%d", &n);
 
    int arr[n];
 
    printf("Enter %d elements in the array: ",n);
    for( i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
    }
 
    printf("Even numbers in the array are: ");
    for( i=0;i<n;i++)
    {
        if(arr[i]%2==0)
            printf("%d ", arr[i]);
    }
 
    printf("\nOdd numbers in the array are: ");
    for( i=0;i<n;i++)
    {
        if(arr[i]%2==1)
            printf("%d ", arr[i]);
    }
    return 0;
}

OUTPUT:

Enter number of elements in the array: 2
Enter 2 elements in the array: 23
24
Even numbers in the array are: 24
Odd numbers in the array are: 23
